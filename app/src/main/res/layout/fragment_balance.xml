<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="@string/frag_balance_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvTitle"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:textSize="30sp"
        android:fontFamily="sans-serif"
        android:textColor="?android:attr/textColorSecondary" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tvTitle"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="@string/frag_balance_earnings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="@+id/view"
        android:layout_marginTop="24dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="78.500,50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvEarnings"
        android:textSize="24sp"
        android:textColor="@color/green_dark_900"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="@string/frag_balance_expenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvExpenses"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:text="68.330,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvExpenses"
        app:layout_constraintLeft_toLeftOf="@+id/tvExpenses"
        android:textColor="@color/red_dark_900"
        android:textSize="24sp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="659dp"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4890511"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="201dp" />

    <TextView
        android:text="@string/frag_balance_future_earnings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textSize="18sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvEarnings"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="@string/frag_balance_future_expenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="@+id/textView14"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="150.000,75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        android:textColor="@color/green_dark_900"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="186dp"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:text="1.250,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:textColor="@color/red_dark_900"
        android:textSize="24sp" />

    <TextView
        android:text="@string/frag_balance_chart_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvChartTitle"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:textColor="?android:attr/textColorSecondary"
        android:textSize="30sp" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvChartTitle"
        android:id="@+id/view5" />

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/llChart"
        android:layout_height="250dp"
        android:layout_width="0dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/tvChartTitle"></LinearLayout>

    <TextView
        android:text="+ 250,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvBalance"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:textColor="@color/green_dark_900"
        android:textSize="30sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal|bold" />

</android.support.constraint.ConstraintLayout>