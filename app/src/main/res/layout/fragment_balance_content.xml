<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp">

    <TextView
        android:text="@string/frag_balance_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvTitle"
        android:textSize="24sp"
        android:fontFamily="sans-serif"
        android:textColor="?android:attr/textColorPrimary"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:id="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tvTitle"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="@string/frag_balance_earnings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textSize="18sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/view"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:fontFamily="sans-serif-light" />

    <TextView
        android:text="0,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvEarnings"
        android:textSize="24sp"
        android:textColor="@color/green_dark_900"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="@string/frag_balance_expenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        android:fontFamily="sans-serif-light" />

    <TextView
        android:text="0,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvExpenses"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:layout_constraintLeft_toLeftOf="@+id/textView20"
        android:textColor="@color/red_dark_900"
        android:textSize="24sp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="556dp"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4890511"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:text="@string/frag_balance_future_earnings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textSize="18sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvEarnings"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:fontFamily="sans-serif-light" />

    <TextView
        android:text="@string/frag_balance_future_expenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="@+id/textView14"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:fontFamily="sans-serif-light" />

    <TextView
        android:text="0,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvEarningsFut"
        android:textColor="@color/green_dark_900"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:text="0,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvExpensesFut"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:textColor="@color/red_dark_900"
        android:textSize="24sp" />

    <TextView
        android:text="@string/frag_balance_chart_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvChartTitle"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/tvEarningsFut"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:textColor="?android:attr/textColorPrimary"
        android:textSize="24sp"
        android:fontFamily="sans-serif" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvChartTitle"
        android:id="@+id/view5" />

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/llChart"
        android:layout_height="250dp"
        android:layout_width="0dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/view5"></LinearLayout>

    <TextView
        android:text="+ 0,00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvBalance"
        android:textColor="@color/green_dark_900"
        android:textSize="24sp"
        android:fontFamily="sans-serif"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/llChart"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="(= ,0,00)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvBalanceFinal"
        app:layout_constraintTop_toBottomOf="@+id/tvBalance"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:textColor="@color/green_dark_900"
        android:textSize="18sp" />


</android.support.constraint.ConstraintLayout>